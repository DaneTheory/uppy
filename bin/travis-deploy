#!/usr/bin/env bash
set -o pipefail
set -o errexit
set -o nounset
# set -o xtrace

echo 'hello from bin/travis-deploy!'
exit 0

git config --global user.name 'Uppy Bot'
git config --global user.email 'uppybot@uppy.io'

# because a Travis deploy script has to be a real file
npm run web:deploy --quiet
